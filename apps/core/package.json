{
  "name": "@casandra/core",
  "version": "0.0.1",
  "description": "",
  "author": "Casandra <https://casandra.ai>",
  "private": true,
  "license": "Commercial",
  "packageManager": "pnpm@8.7.6",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write ",
      "eslint --cache --fix"
    ]
  },
  "bump": {
    "before": [
      "git pull --rebase"
    ]
  },
  "scripts": {
    "build": "nest build --webpack",
    "dev": "npm run start",
    "start": "cross-env NODE_ENV=development nest start -w",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "npm run prism:migrate:deploy && cross-env NODE_ENV=production node dist/main.js",
    "prod:debug": "cross-env NODE_ENV=production nest start --debug --watch"
  },
  "dependencies": {
    "@fastify/swagger": "9.6.1",
    "@fastify/swagger-ui": "5.2.3",
    "@fastify/cors": "11.1.0",
    "@db/client": "workspace:@casandra/prisma@*",
    "@nestjs/cache-manager": "3.0.1",
    "@nestjs/common": "11.1.9",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.9",
    "@nestjs/event-emitter": "3.0.1",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-fastify": "11.1.9",
    "@nestjs/platform-socket.io": "11.1.9",
    "@nestjs/schedule": "6.0.1",
    "@nestjs/swagger": "11.2.3",
    "@nestjs/throttler": "6.4.0",
    "@nestjs/websockets": "11.1.9",
    "axios": "1.6.2",
    "bcrypt": "5.1.1",
    "cache-manager": "7.2.5",
    "cache-manager-ioredis": "2.1.0",
    "keyv": "5.5.4",
    "dotenv": "16.4.5",
    "cron": "^3.1.6",
    "cross-env": "7.0.3",
    "dayjs": "1.11.10",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "nestjs-pretty-logger": "0.2.0",
    "nestjs-zod": "5.0.1",
    "redis": "4.6.11",
    "reflect-metadata": "0.1.14",
    "rxjs": "7.8.2",
    "slugify": "1.6.6",
    "snakecase-keys": "5.5.0",
    "zod": "4.1.12",
    "zx-cjs": "7.0.7-0"
  },
  "devDependencies": {
    "@nestjs/cli": "11.0.12",
    "@nestjs/schematics": "11.0.9",
    "@types/bcrypt": "5.0.2",
    "@types/cache-manager": "5.0.0",
    "@types/jsonwebtoken": "9.0.5",
    "@types/supertest": "2.0.16",
    "@types/ua-parser-js": "0.7.39",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "zx": "7.2.3"
  },
  "redisMemoryServer": {
    "downloadDir": "./tmp/redis/binaries",
    "version": "6.0.10",
    "disablePostinstall": "1",
    "systemBinary": "/opt/homebrew/bin/redis-server"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}