{
  "name": "@casandra-ai/shared-contracts",
  "version": "0.3.10",
  "private": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.mjs",
      "require": "./dist/*.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "build": "pnpm run clean && tsup src/index.ts --format cjs,esm --dts --out-dir dist --clean",
    "clean": "rimraf dist tsconfig.tsbuildinfo"
  },
  "peerDependencies": {
    "zod": "^3.23.8 || ^4.0.0"
  },
  "devDependencies": {
    "rimraf": "^5.0.0",
    "tsup": "^8.0.1",
    "typescript": "^5.4.5",
    "zod": "^4.0.0",
    "@types/fhir": "^0.0.41"
  }
}